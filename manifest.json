{
    "arguments": {
        "install": [
            {
                "ask": {
                    "en": "Choose a domain name for Kresus",
                    "fr": "Choisissez un nom de domaine pour Kresus"
                },
                "example": "example.com",
                "name": "domain",
                "type": "domain"
            },
            {
                "ask": {
                    "en": "Choose a path for Kresus",
                    "fr": "Choisissez un chemin pour Kresus"
                },
                "default": "/kresus",
                "example": "/kresus",
                "name": "path",
                "type": "path"
            },
            {
                "ask": {
                    "en": "Choose a user",
                    "fr": "Choisissez un utilisateur"
                },
                "example": "Toto",
                "name": "user",
                "type": "user"
            },
            {
                "ask": {
                    "en": "Would you like to allow email notifications ?",
                    "fr": "Voulez-vous autoriser les notifications par email ?"
                },
                "name": "setup_email",
                "type": "boolean"
            }
        ]
    },
    "description": {
        "en": "Kresus is a personal finance manager.",
        "fr": "Kresus est un outil personnel de gestion de finances."
    },
    "id": "kresus",
    "license": "free",
    "maintainer": {
        "name": "Scith",
        "url": "https://github.com/scith"
    },
    "multi_instance": true,
    "name": "Kresus",
    "packaging_format": 1,
    "requirements": {
        "yunohost": ">= 2.6.4"
    },
    "services": [
        "nginx"
    ],
    "url": "https://framagit.org/bnjbvr/kresus"
}
